{
  "name": "tslane",
  "version": "1.3.0",
  "description": "A template for creating TypeScript libraries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "bundle": "npx webpack --config-name=production",
    "bundle:all": "npx webpack",
    "bundle:map": "npx webpack --config-name=map",
    "coverage": "npx jest --collectCoverage",
    "doc": "npx typedoc --out docs src",
    "format": "npx prettier --parser typescript --write ./src ./tests",
    "lint": "npx eslint . --ext .ts",
    "test": "npx jest"
  },
  "keywords": [],
  "author": "",
  "license": "GPL3",
  "devDependencies": {
    "@types/jest": "^29.4.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "npm-dts-webpack-plugin": "^1.3.12",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.26",
    "typedoc-plugin-katex": "^0.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1"
  }
}
