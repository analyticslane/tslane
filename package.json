{
  "name": "tslane",
  "version": "1.2.0",
  "description": "A template for creating TypeScript libraries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "bundle": "npx webpack --config-name=production",
    "bundle:all": "npx webpack",
    "bundle:map": "npx webpack --config-name=map",
    "coverage": "npx jest --collectCoverage",
    "doc": "typedoc --out docs src",
    "format": "npx prettier --parser typescript --write ./src ./tests",
    "lint": "npx eslint . --ext .ts",
    "test": "npx jest"
  },
  "keywords": [],
  "author": "",
  "license": "GPL3",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "jest": "^27.0.5",
    "npm-dts-webpack-plugin": "^1.3.7",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  }
}
